{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "jsRules": {
    "no-unused-expression": true
  },
  "rules": {
    "interface-name": false,
    "member-ordering": false,
    "no-console": false,
    "object-literal-sort-keys": false,
    "quotemark": [true, "avoid-escape", "avoid-template"],
    "arrow-parens": false
  },
  "rulesDirectory": []
}
